{
  "name": "getbike.online-server",
  "version": "1.0.0",
  "private": true,
  "author": "Miloš Tomšik",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node",
    "build": "babel server.js -d dist",
    "serve": "node dist/server.js",
    "seed:users": "babel-node ./services/seeder/executables/users.js",
    "seed:categories": "babel-node ./services/seeder/executables/categories.js",
    "seed:subcategories": "babel-node ./services/seeder/executables/subcategories.js",
    "seed:ads": "babel-node ./services/seeder/executables/ads.js",
    "seed:ratings": "babel-node ./services/seeder/executables/ratings.js",
    "seed:notifications": "babel-node ./services/seeder/executables/notifications.js",
    "seed:messages": "babel-node ./services/seeder/executables/messages.js",
    "seed:threads": "babel-node ./services/seeder/executables/threads.js",
    "seed:all": "babel-node ./services/seeder/executables/all.js",
    "seed": "babel-node ./services/seeder/executables/all.js"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.18.0",
    "mongoose": "^4.13.9",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "faker": "^4.1.0",
    "nodemon": "^1.14.11"
  }
}
