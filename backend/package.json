{
  "name": "getbike.online-server",
  "version": "1.0.0",
  "private": true,
  "author": "Miloš Tomšik",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "build": "babel server.js -d dist --presets es2015,stage-2",
    "serve": "node dist/server.js",
    "seed": "babel-node ./services/seed-list.js --presets es2015,stage-2",
    "seed:users": "babel-node ./services/seeder/users.js --presets es2015,stage-2",
    "seed:ads": "babel-node ./services/seeder/ads.js --presets es2015,stage-2",
    "seed:ratings": "babel-node ./services/seeder/ratings.js --presets es2015,stage-2",
    "seed:notifications": "babel-node ./services/seeder/notifications.js --presets es2015,stage-2",
    "seed:categories": "babel-node ./services/seeder/categories.js --presets es2015,stage-2",
    "seed:subcategories": "babel-node ./services/seeder/subcategories.js --presets es2015,stage-2",
    "seed:messages": "babel-node ./services/seeder/messages.js --presets es2015,stage-2",
    "seed:threads": "babel-node ./services/seeder/threads.js --presets es2015,stage-2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphiql": "^0.11.10",
    "graphql": "^0.11.7",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "faker": "^4.1.0",
    "nodemon": "^1.12.5"
  }
}
